import javax.swing.JOptionPane;
import java.util.Scanner;

public class text{
  public static void main(String[] args){
	String a1 = JOptionPane.showInputDialog(null,"请输入a值:");
	String b1 = JOptionPane.showInputDialog(null,"请输入b值:");
	String c1 = JOptionPane.showInputDialog(null,"请输入c值:");
	double a = Double.parseDouble(a1);
	double b = Double.parseDouble(b1);
	double c = Double.parseDouble(c1);
	System.out.printf("%.1fx^2 + %.1fx + %.1f = 0\n",a,b,c);
	if(b * b - 4 * a * c < 0)
		System.out.println("方程式无实根");
	else if(b * b - 4 * a * c==0)
		System.out.printf("r1=r2=%.1f\n",(-b + Math.pow(b * b - 4 * a * c,0.5)) / 2 * a);
	else
		System.out.printf("r1 = %.1f\nr2 = %f\n",(-b - Math.pow(b * b - 4 * a * c,0.5)) / 2 * a,(-b + Math.pow(b * b - 4 * a * c,0.5)) / 2 * a);
  }
}
